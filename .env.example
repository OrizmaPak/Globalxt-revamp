VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=

# Auto seed local content into Firestore on first run (true/false)
VITE_AUTO_SEED=false

# Cloudinary settings (Optional - now using Dropbox for file storage)
VITE_CLOUDINARY_CLOUD_NAME=duoojkl6a
VITE_CLOUDINARY_UPLOAD_PRESET=

# Dropbox file storage settings (CLIENT-SIDE ONLY - NO BACKEND!)
# Get access token from your Dropbox App Console at https://www.dropbox.com/developers/apps
# This token is used directly from your React app - no server needed!
VITE_DROPBOX_ACCESS_TOKEN=

# Admin email allowlist (comma-separated). Defaults to orevaorior@gmail.com if not set
VITE_ADMIN_EMAILS=

# Public site origin used for canonical URLs and sitemap
# Example: https://globalxtltd.com
VITE_SITE_URL=
